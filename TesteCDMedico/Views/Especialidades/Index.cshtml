@model IEnumerable<TesteCDMedico.ESPECIALIDADES>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Adicionar Especialidade", "Adicionar")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Especialidade)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Especialidade)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id=item.IDEspecialidades }) |
                @Html.ActionLink("Details", "Details", new { id=item.IDEspecialidades }) |
                @Html.ActionLink("Excluir", "Excluir", new { id=item.IDEspecialidades })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
