@model IEnumerable<TesteCDMedico.MEDICOS>

@{
    ViewBag.Title = "Index";
}

<h2>Cade Meu Medico?</h2>

<p>
    @Html.ActionLink("Adicionar Medico", "Adicionar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CRM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endereco)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bairro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Convenio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Clinica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Website)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CIDADES.Cidade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ESPECIALIDADES.Especialidade)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {  
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CRM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endereco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bairro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Convenio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Clinica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Website)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CIDADES.Cidade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ESPECIALIDADES.Especialidade)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.IDMedico }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDMedico }) |
            @Html.ActionLink("Excluir", "Excluir", new { id=item.IDMedico })
        </td>
    </tr>
}

</table>
